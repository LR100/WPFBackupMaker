<UserControl x:Class="WPFBackupMaker.Views.UIKitMoleculesView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFBackupMaker.Views"
             xmlns:vms="clr-namespace:WPFBackupMaker.ViewModels"
             xmlns:cc="clr-namespace:WPFBackupMaker.UIKit"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=vms:UIKitOrganismsViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="{StaticResource Body.Margin}">
        <DockPanel>
            <StackPanel>
                <cc:UINumberBox Step="1"/>
            </StackPanel>
        </DockPanel>
    </Grid>
</UserControl>
